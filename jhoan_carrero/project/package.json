{
  "name": "emoji-voter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prepare": "npm run install:server && npm run install:client",
    "install:server": "cd server && npm install --omit=dev",
    "install:client": "cd client && npm install --omit=dev",
    "start": "npm run start:server",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "start:server": "cd server && npm start",
    "dev": "concurrently npm:dev:*",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "lint:fix": "npx eslint --fix client",
    "test": "npm run test:main && npm run test:server && npm run test:client",
    "test:main": "jest main.test.js",
    "test:server": "jest server.test.js",
    "test:client": "cypress run --spec **/client.test.js",
    "cypress": "cypress",
    "cypress:open": "cypress open",
    "jest": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhoacar/emoji-voter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jhoacar/emoji-voter/issues"
  },
  "homepage": "https://github.com/jhoacar/emoji-voter#readme",
  "devDependencies": {
    "axios": "^1.1.3",
    "cli-spinners": "^2.7.0",
    "colors": "^1.4.0",
    "concurrently": "^7.4.0",
    "cypress": "^10.11.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.1",
    "mongoose": "^6.6.5",
    "uuidv4": "^6.2.13"
  }
}
