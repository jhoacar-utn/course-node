{
  "type": "module",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "husky install",
    "test": "npm run test:client && npm run test:server && npm run test:main",
    "test:main": "jest main.test.js --colors",
    "test:server": "jest server.test.js --colors",
    "test:client": "cypress run --spec **/client.test.js",
    "cypress": "cypress",
    "cypress:reinstall": "cypress cache clear && cypress install --force",
    "cypress:open": "cypress open",
    "jest": "jest",
    "evaluation": "node project",
    "evaluate": "node project",
    "evaluate:main": "cross-env DEBUG_TEST='' npm run test:main -- --silent 2>&1",
    "evaluate:server": "cross-env DEBUG_TEST='' npm run test:server -- --silent 2>&1",
    "evaluate:client": "cross-env DEBUG_TEST='' npm run test:client --prefix=${ROOT_PATH} -- -q --config video=false,screenshotOnRunFailure=false 2>&1",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:kill:containers": "docker ps -aq | xargs docker kill",
    "docker:rm:containers": "docker ps -aq | xargs docker rm",
    "docker:rm:images": "docker images -aq | xargs docker rmi ",
    "docker:rm:volumes": "docker volume ls -q | xargs docker volume rm",
    "docker:rm:all": "docker system prune",
    "docker:in": "docker exec -it utn bash",
    "clear:node_modules": "find . -name node_modules | xargs rm -r",
    "clear:package-lock.json": "find . -name package-lock.json | xargs rm",
    "port:scanner": "node project/scanner.mjs",
    "send:mail": "node project/gmail/send.mjs"
  },
  "devDependencies": {
    "await-event-emitter": "^2.0.2",
    "axios": "^1.1.3",
    "cli-spinners": "^2.7.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.11.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "mongoose": "^6.6.5",
    "nodemailer": "^6.8.0",
    "react": "^18.2.0",
    "uuidv4": "^6.2.13"
  }
}
