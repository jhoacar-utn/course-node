        "install:server": "cd server && npm install",
        "install:client": "cd client && npm install"
    },
    "devDependencies": {
        "concurrently": "^7.6.0"
    },
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.10.16",
        "@mui/material": "^5.10.17"
    }
}
